{
  "name": "vyakui-react",
  "private": false,
  "version": "1.3.1",
  "description": "React/Next.js component library ",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    },
    "./global.css": "./dist/index.css"
  },
  "scripts": {
    "build": "tsup src/index.ts --format cjs,esm --dts --minify --clean",
    "prepublishOnly": "npm run build",
    "dev": "tsup src/index.ts --format cjs,esm --watch --dts",
    "lint": "tsc"
  },
  "keywords": [
    "react",
    "typescript",
    "polymorphic",
    "styled"
  ],
  "author": "vyaknil",
  "license": "ISC",
  "type": "commonjs",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/vyaknil/vyakui-react.git"
  },
  "peerDependencies": {
    "react": "^19",
    "react-dom": "^19"
  },
  "devDependencies": {
    "@types/node": "^25.2.0",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "react": "^19",
    "tsup": "^8.5.1",
    "typescript": "^5.9.3"
  }
}
