{
  "name": "vyakui-react",
  "version": "1.3.6",
  "description": "React/Next.js component library",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    },
    "./global.css": "./dist/index.css"
  },
  "scripts": {
    "build": "tsup && tsc --project tsconfig.build.json --noEmit",
    "type-check": "tsc --noEmit",
    "dev": "tsup --watch",
    "lint": "tsc --noEmit",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "react",
    "typescript",
    "polymorphic",
    "styled",
    "nextjs"
  ],
  "author": "vyaknil",
  "license": "ISC",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/vyaknil/vyakui-react.git"
  },
  "peerDependencies": {
    "react": "^18.0.0 || ^19.0.0",
    "react-dom": "^18.0.0 || ^19.0.0"
  },
  "devDependencies": {
    "@types/react": "^18.0.0 || ^19.0.0",
    "@types/react-dom": "^18.0.0 || ^19.0.0",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "tsup": "^8.5.1",
    "typescript": "^5.9.3"
  },
  "publishConfig": {
    "access": "public"
  }
}
